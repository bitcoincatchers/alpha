    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "ed25519-hd-key": "^1.3.0",
    "express": "^4.18.2",
    "hono": "^4.0.0",
    "node-fetch": "^2.7.0",
    "sqlite3": "^5.1.6",
    "telegraf": "^4.12.2",
    "tweetnacl-util": "^0.15.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250705.0",
    "@hono/vite-cloudflare-pages": "^0.4.2",
