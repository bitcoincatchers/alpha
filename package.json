{
  "name": "alphabot",
  "version": "2.0.0",
  "description": "AlphaBot - Professional Trading Signals Dashboard with ROI Tracking",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "vite",
    "build": "vite build",
    "bot": "node bot.js",
    "server": "node server.js",
    "test": "curl http://localhost:3000/api/health",
    "clean-port": "fuser -k 3000/tcp 2>/dev/null || true",
    "setup": "npm install && npm run clean-port && pm2 start ecosystem.config.cjs",
    "restart": "pm2 restart ecosystem.config.cjs",
    "stop": "pm2 stop ecosystem.config.cjs",
    "logs": "pm2 logs --nostream",
    "status": "pm2 list"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.11.0",
